{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch", 
      "name": "Launch chrome against localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src"
    },
    {
      "type": "pwa-chrome",
      "request": "attach", 
      "port": "9222",
      "name": "attach to chrome",
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}